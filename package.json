{
    "name": "nuvio-providers",
    "version": "1.0.0",
    "description": "Providers for Nuvio Streaming",
    "author": "paregi12",
    "scripts": {
        "build": "node --max-old-space-size=4096 build.js",
        "build:watch": "nodemon --watch src -e js,json --exec 'node --max-old-space-size=4096 build.js'",
        "build:provider": "node --max-old-space-size=4096 build.js",
        "start": "node --max-old-space-size=4096 server.js"
    },
    "devDependencies": {
        "cheerio": "^1.1.2",
        "cheerio-without-node-native": "^0.20.2",
        "esbuild": "^0.20.0",
        "nodemon": "^3.0.0"
    },
    "keywords": [
        "nuvio",
        "streaming",
        "providers"
    ],
    "license": "GPL-3.0",
    "dependencies": {
        "axios": "^1.13.2",
        "crypto-js": "^4.2.0"
    }
}
